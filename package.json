{
  "name": "installer",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "levitate-docs": "./dist/index.js"
  },
  "scripts": {
    "dev": "bun src/index.tsx",
    "build": "bun build src/index.tsx --outdir dist --target node --format esm --external ink --external react --external @levitate/docs-content",
    "typecheck": "tsc --noEmit",
    "start": "./bin/levitate-installer"
  },
  "dependencies": {
    "@levitate/docs-content": "workspace:*",
    "ink": "^5.2.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
